{
    "name": "stansberry-investor-hour",
    "author": "Stansberry Research Web Team",
    "version": "0.0.0",
    "description": "Stansberry Investor Hour",
    "homepage": "https://github.com/Stansberry-Holdings/Stansberry-Research-Investor-Hour#README",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Stansberry-Holdings/Stansberry-Research-Investor-Hour.git"
    },
    "bugs": {
        "url": "https://github.com/Stansberry-Holdings/Stansberry-Research-Investor-Hour/issues"
    },
    "keywords": [
        "Stansberry",
        "Investor",
        "Hour"
    ],
    "main": "app.js",
    "scripts": {
        "debug": "npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/app.js",
        "start": "node dist/app.js",
        "test": "mocha \"test/*.test.ts\" --require ts-node/register --reporter spec --bail false --exit",
        "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "watch-node": "nodemon dist/app.js",
        "watch-sass": "node-sass -w src/scss/default.scss dist/public/assets/styles/default.css",
        "watch-test": "npm run test -- --watchAll",
        "watch-ts": "tsc -w"
    },
    "dependencies": {
        "body-parser": "1.18.3",
        "compression": "1.7.3",
        "cors": "2.8.4",
        "dotenv": "6.0.0",
        "ejs": "2.6.1",
        "express": "4.16.3",
        "helmet": "3.12.1",
        "moment": "2.22.2",
        "moment-timezone": "0.5.21",
        "mongoose": "5.2.4",
        "morgan": "1.9.0",
        "redis": "2.8.0",
        "rss": "1.2.2",
        "winston": "3.0.0"
    },
    "devDependencies": {
        "@types/bluebird": "3.5.22",
        "@types/chai": "4.1.4",
        "@types/compression": "0.0.36",
        "@types/dotenv": "4.0.3",
        "@types/ejs": "2.6.0",
        "@types/express": "4.16.0",
        "@types/helmet": "0.0.38",
        "@types/mocha": "5.2.5",
        "@types/moment-timezone": "0.5.6",
        "@types/mongoose": "5.2.0",
        "@types/morgan": "1.7.35",
        "@types/node": "10.5.2",
        "@types/redis": "2.8.6",
        "@types/rss": "0.0.28",
        "@types/shelljs": "0.8.0",
        "@types/winston": "2.3.9",
        "bootstrap": "4.1.2",
        "chai": "4.1.2",
        "concurrently": "3.6.0",
        "fancy-log": "1.3.2",
        "gulp": "4.0.0",
        "gulp-autoprefixer": "5.0.0",
        "gulp-minify": "3.1.0",
        "gulp-mocha": "6.0.0",
        "gulp-nodemon": "2.2.1",
        "gulp-sass": "4.0.1",
        "gulp-sourcemaps": "2.6.4",
        "gulp-tslint": "8.1.3",
        "gulp-typescript": "4.0.2",
        "headroom.js": "0.9.4",
        "jquery": "3.3.1",
        "merge2": "1.2.2",
        "mocha": "5.2.0",
        "node-sass": "4.9.2",
        "nodemon": "1.18.2",
        "pikaday": "1.7.0",
        "popper.js": "1.14.3",
        "supertest": "3.1.0",
        "ts-node": "7.0.0",
        "tslint": "5.11.0",
        "typescript": "2.9.2"
    }
}
